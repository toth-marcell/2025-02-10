{
  "type": "module",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@awmottaz/prettier-plugin-void-html": "^1.7.0",
    "prettier": "^3.5.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sqlite3"
    ]
  },
  "prettier": {
    "plugins": [
      "@awmottaz/prettier-plugin-void-html"
    ],
    "overrides": [
      {
        "files": [
          "*.ejs"
        ],
        "options": {
          "parser": "html"
        }
      }
    ]
  }
}
